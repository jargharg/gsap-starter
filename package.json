{
	"name": "gsap",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "npm run serve",
		"serve": "vue-cli-service serve --open",
		"build": "vue-cli-service build",
		"deploy": "npm run build && firebase deploy"
	},
	"dependencies": {
		"gsap": "^2.0.2",
		"vue": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.0",
		"@vue/cli-service": "^3.0.0",
		"eslint": "^5.7.0",
		"eslint-plugin-vue": "^4.7.1",
		"node-sass": "^4.9.0",
		"sass-loader": "^7.0.1",
		"vue-template-compiler": "^2.5.17"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"babel": {
		"presets": [
			"@vue/app"
		]
	},
	"eslintConfig": {
		"rules": {
			"import/no-unresolved": "off"
		},
		"extends": [
			"plugin:vue/essential"
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 10"
	]
}
